{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "text",
      "required": true
    },
    "precio": {
      "type": "decimal",
      "required": true,
      "min": 1
    },
    "stock": {
      "type": "biginteger",
      "required": true
    },
    "fk_idProveedor": {
      "type": "relation",
      "required": true,
      "relation": "manyToOne",
      "target": "api::proveedor.proveedor"
    },
    "categorias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categoria.categoria",
      "mappedBy": "productos"
    }
  }
}
